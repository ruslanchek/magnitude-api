{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": false,
    "removeComments": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "target": "es2016",
    "sourceMap": true,
    "rootDir": "src",
    "outDir": "build",
    "incremental": false,
    "strict": false
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "build"]
}
